{
  "name": "delete-test-01",
  "description": "delete-test-01-description",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.398.0",
    "@aws-sdk/lib-dynamodb": "^3.398.0",
    "@stak/shared": "file:../shared/"
  },
  "scripts": {
    "dev": "sam local start-api --skip-pull-image",
    "dev:info": "awslocal apigateway get-rest-apis | jq '.items[] | \"http://localhost:4566/_aws/execute-api/\\(.id)/Prod/user/message\"'",
    "delete": "samlocal delete --region us-east-1 --stack-name local --no-prompts",
    "sync": "samlocal deploy --region us-east-1 --stack-name local",
    "build": "sam build && sam validate",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "aws-sdk-client-mock": "^2.0.0",
    "esbuild": "^0.25.8",
    "jest": "^29.2.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  }
}
